
@{
    ViewBag.Title = "AdminForm";
}

<h2>AdminForm</h2>
<table class="col-md-3" id="ProductBox" style="padding: 2px;">
    <tr>
        <th>Name </th>
        <th>Description</th>
        <th>Quantity</th>
        <th>Price</th>
        <th></th>
        <th></th>
    </tr>
    @foreach (var item in ViewBag.ItemsList)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Description</td>
            <td>@item.Quantity</td>
            <td>@item.Price</td>
            <td>Edit Link</td>
            <td>Delete Link</td>
           


        </tr>

    }
    


</table>


